{
  "name": "op-shopper",
  "version": "0.1.4",
  "description": "App for op shops to list items online for customers to browse.",
  "main": "index.js",
  "scripts": {
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "seed": "yarn knex seed:run",
    "migrate": "yarn knex migrate:latest",
    "rollback": "yarn knex migrate:rollback",
    "dev": "nodemon index",
    "start": "node server & webpack --config client/webpack.config.js --watch",
    "test": "jest --watch --noStackTrace"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  },
  "author": "Alisa Herrera-Hayman",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "knex": "^0.15.2",
    "nodemon": "^1.18.3",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.0",
    "cheerio": "0.22.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "jest": "^23.5",
    "react": "^16.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "supertest": "^3.1.0",
    "webpack": "^4.17.1",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.7"
  }
}
